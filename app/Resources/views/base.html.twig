<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Pro3x Online{% endblock %}</title>
        {% block stylesheets %}{% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
	<link rel="stylesheet" type="text/css" href="{{ asset('styles/simple.css') }}" />
    </head>
    <body>
	{% block head %}
	    <div class="pro3x_header">
		<a href="{{ path('home') }}">Pro3x Online</a>
	    </div>
	{% endblock %}
	    
	<div class="pro3x_page_title_box">
	    {% block page_title %}{% endblock %}
	    <div class="clear"></div>
	</div>
	    
	<div class="container_12 pro3x_content">
	    {% block body %}{% endblock %}
	    <div class="clear"></div>
	</div>

        {% block javascripts %}{% endblock %}
    </body>
</html>
