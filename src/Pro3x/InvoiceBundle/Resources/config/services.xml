<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    
    <parameters>
        <parameter key="fina.wdsl">http://localhost/pro3x/fina/wsdl/FiskalizacijaService.wsdl</parameter>
    </parameters>
    
    
    <services>
        <service id="fina.client.factory" class="Pro3x\Online\FinaClientFactory">
            <argument>%fina.wsdl%</argument>
        </service>
	<service id="error.handler" class="Pro3x\Online\ErrorHandler" />
    </services>
    
</container>
